{
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["go", "json"],

  // jump to definition/impl in new or existing tab
  "coc.preferences.jumpCommand": "drop",

  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  }
}
